{
  "manifest_version": 3,
  "name": "DofusWeb",
  "version": "1.0",
  "description": "Rassemble tous les liens utiles pour Dofus",
  "browser_specific_settings": {
    "gecko": {
      "id": "dofusweb@extension",
      "strict_min_version": "109.0"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
} 